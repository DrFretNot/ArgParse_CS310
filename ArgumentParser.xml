<?xml version="1.0"?>
<program>
<name>VolumeCalculator</name>
<description>Calculate the volume of an object.</description>
<arguments>
<positional>
<name>length</name>
<type>float</type>
<description>the length of the object</description>
<position>1</position>
<valueset>7.0,8.0,9.0,</valueset>
</positional>
<positional>
<name>width</name>
<type>float</type>
<description>the width of the object</description>
<position>2</position>
</positional>
<positional>
<name>height</name>
<type>float</type>
<description>the height of the object</description>
<position>3</position>
</positional>
<named>
<name>type</name>
<shortname>t</shortname>
<type>string</type>
<description>the shape of the object</description>
<default>box</default>
</named>
<named>
<name>digits</name>
<shortname>d</shortname>
<type>integer</type>
<description>the number of decimal digits to truncate at</description>
<default>4</default>
</named>
</arguments>
</program>
